library(tidyverse)
library(ggplot2)
data(iris)
head(iris)
str(iris)
summary(iris)
ggplot(iris, aes(x = Sepal.Length, y = Sepal.Width, color = Species)) +
geom_point() +
theme_minimal()
ggplot(iris, aes(x = Petal.Length, y = Petal.Width, color = Species)) +
geom_point() +
theme_minimal()
iris %>%
group_by(Species) %>%
summarise(across(where(is.numeric), mean, .names = "mean_{.col}"))
data(iris) #데이터 불러오기
force(iris)
iris
str(iris)
summary(iris)
library(dplyr)
library(tidyverse)
library(ggplot2)
library(dplyr)
iris %>%
group_by(Species) %>%
summarise(across(where(is.numeric),
list(Min = min, Median = median, Mean = mean, Max = max)))
# setosa 데이터만 추출
setosa_data <- iris %>% filter(Species == "setosa")
# versicolor 데이터만 추출
versicolor_data <- iris %>% filter(Species == "versicolor")
# virginica 데이터만 추출
virginica_data <- iris %>% filter(Species == "virginica")
# setosa 요약 통계
summary(setosa_data)
# versicolor 요약 통계
summary(versicolor_data)
# virginica 요약 통계
summary(virginica_data)
# 꽃잎 길이 (Petal.Length) 기준으로 시각화
ggplot(iris, aes(x = Petal.Length, fill = Species)) +
geom_histogram(aes(y = ..density..),
position = "identity",
alpha = 0.5,
bins = 30,
color = "black") +  # 히스토그램
geom_density(alpha = 0.7) +  # 밀도곡선 (선모양)
labs(title = "꽃잎 길이 분포 (전체 데이터)",
x = "꽃잎 길이 (cm)",
y = "밀도 (Density)") +  # 제목과 축 라벨
theme_minimal(base_size = 14) +  # 깔끔한 테마
scale_fill_brewer(palette = "Set2")  # 색상 팔레트
# 패키지 불러오기
library(ggplot2)
# 전체 데이터 기준 (꽃잎 길이)
ggplot(iris, aes(x = Petal.Length)) +
geom_histogram(aes(y = ..density..),
bins = 30,
fill = "skyblue",
color = "black",
alpha = 0.7) +  # 히스토그램
geom_density(color = "darkblue",
size = 1) +  # 밀도곡선 (선 모양)
labs(title = "전체 붓꽃 데이터의 꽃잎 길이 분포",
x = "꽃잎 길이 (cm)",
y = "밀도 (Density)") +  # 제목과 축 이름
theme_minimal(base_size = 14)  # 깔끔한 테마
# 패키지 불러오기
library(ggplot2)
# 꽃 종류별로 시각화 (꽃잎 길이)
ggplot(iris, aes(x = Petal.Length, fill = Species)) +
geom_histogram(aes(y = ..density..),
position = "identity",
alpha = 0.5,
bins = 30,
color = "black") +  # 히스토그램
geom_density(aes(color = Species),
size = 1.2) +  # 밀도곡선 (꽃 종류별 색상)
